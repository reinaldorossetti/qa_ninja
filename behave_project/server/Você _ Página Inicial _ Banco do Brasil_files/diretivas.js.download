												$('.callToAction').css("display", "none");
											} else {
												$('.callToAction').css("display", "block");
											}
										}
									});
									mapeado = true;
								}
							}
							rolagemCallToAction();
							$(window).resize(function () {
								if (typeof window.timeResize == 'undefined')
									window.timeResize = setTimeout(function () {}, 0);
									clearTimeout(window.timeResize);
