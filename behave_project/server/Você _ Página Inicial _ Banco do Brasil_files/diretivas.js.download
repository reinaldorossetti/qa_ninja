			          		}
			          	}
			          	if(typeof scope.data.chamada1.enderecoPaginaChamada !== 'undefined') {
			          		if(scope.data.chamada1.enderecoPaginaChamada.trim() != '') {
			          			var textoLink = $filter('getTextoLink')(scope.data.chamada1.texto);
